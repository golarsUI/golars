<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.customerportal.bean.InventoryReport" table="inventoryreport__c">
 		<id name="product">
			<column name="PRODUCT__c" />
		</id>
  <property name="dateTime" column="DATETIME"  />
  <property name="gallons" column="GALLONS__c"  />
  <property name="tank" column="TANK__c"  />		
 </class>
</hibernate-mapping>