<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.customerportal.bean.Company" table="Affiliate_Company__c">
  <id name="companyName">
   <column name="Company_Name__c"  />
  </id>
  <property name="companyOwner" column="Company_Owner__c" />
  <property name="existingClient" column="Existing_Client__c" />
  <property name="externalID" column="External_ID__c" />
  <property name="name" column="Company__c"/>
  <property name="ownerName" column="Owner_Name__c"/>
      <property name="companyAddress" column="Company_Address__c"/>
    <property name="company" column="Name"/>
  
 </class>
</hibernate-mapping>