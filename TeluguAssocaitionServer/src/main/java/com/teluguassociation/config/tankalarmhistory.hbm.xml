<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.customerportal.bean.TankAarmHistory" table="tankalarmhistory__c">
 		<id name="id">
			<column name="Id" />
		</id>
  <property name="type" column="Type__c"  />
  <property name="tank" column="TANK__c"  />
  <property name="runDate" column="RunDate__c"  />	
  <property name="occuredDate" column="OccuredDate__c"  />	
  <property name="fid" column="FID__c"  />	
  <property name="facility" column="Facility__c"  />	
  <property name="alarmType" column="AlarmType__c"  />	
  <property name="name" column="Name"  />	
   <property name="viewed" column="Viewed"  />	
 </class>
</hibernate-mapping>