<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.customerportal.bean.Contact" table="Contact">
  <id name="id">
   <column name="Id"  />
  </id>
  
  <property name="existingClient" column="Existing_Client__c"/>
   <property name="externalID" column="External_ID__c"/>
  <property name="firstName" column="FirstName"/>	
  <property name="lastName" column="LastName"/>
  <property name="emailAddress" column="Email__c"/>
  <property name="mailingAddress" column="MailingAddress"/>
  <property name="mailingCity" column="MailingCity"/>
  <property name="mailingCountry" column="MailingCountry"/>
  <property name="mailingPostalCode" column="MailingPostalCode"/>
  <property name="mailingState" column="MailingState"/>
  <property name="mailingStreet" column="MailingStreet"/>
  <property name="memberID" column="Member_ID__c"/>
  <property name="middleName" column="MiddleName"/>
  <property name="mobilePhone" column="MobilePhone"/>
  <property name="name" column="Name"/>
  <property name="phone" column="Phone"/>
  <property name="profession" column="Profession__c"/>
  <property name="salutation" column="Salutation"/>
  <property name="taxID" column="Tax_ID__c"/>
  <property name="title" column="Title"/>

 </class>
</hibernate-mapping>