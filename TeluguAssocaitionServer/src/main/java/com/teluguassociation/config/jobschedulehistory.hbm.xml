<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.customerportal.bean.JobScheduleHistory" table="schedulejobhistory">
		<id name="id">
			<column name="Id" />
		</id>
		<property name="jobName" column="Name" />
		<property name="jobExeuctionTime" column="jobexecutiontime" />
		<property name="result" column="result" />
	</class>
</hibernate-mapping>