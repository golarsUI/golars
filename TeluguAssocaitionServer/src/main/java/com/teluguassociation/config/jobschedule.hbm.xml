<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.customerportal.bean.JobSchedule" table="schedulejob">
		<id name="jobName">
			<column name="Name" />
		</id>
		<property name="dependentJobName" column="dependentjob" />
		<property name="jobPath" column="jobpath" />
		<property name="sourceFilePath" column="sourcefile
		" />
		<property name="endFilePath" column="endfilename" />
		<property name="startTime" column="startTime" />
		<property name="schedule" column="schedule" />
		<property name="recurrence" column="recurrence" />
	</class>
</hibernate-mapping>